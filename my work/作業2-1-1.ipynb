   ],
   "source": [
    "n=int(input())\n",
    "a=\"*\"\n",
    "b=\" \"\n",
    "c=1\n",
    "d=0\n",
    "for i in range(0,n//2):\n",
    "    print(b*i+a+b*(n-2*c)+a)\n",
    "    c=c+1\n",
    "print(b*(n//2)+a)\n",
    "for j in range(n//2-1,-1,-1):\n",
    "    print(b*j+a+b*(1+2*d)+a)\n",
    "    d=d+1"
   ]
  }
